<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    
    <display-name>Bill Dauterive's Army Surplus &amp; Barber Shop</display-name>
    <description>King of the Hill CTF Challenge - Ghostcat Vulnerability (Bill Dauterive Theme)</description>
    
    <context-param>
        <param-name>shop-owner</param-name>
        <param-value>William "Bill" Dauterive</param-value>
    </context-param>
    
    <context-param>
        <param-name>army-unit</param-name>
        <param-value>Former Army Sergeant - Operation Lone Star</param-value>
    </context-param>
    
    <context-param>
        <param-name>database-url</param-name>
        <param-value>jdbc:mysql://db:3306/ctf_db</param-value>
    </context-param>
    
    <context-param>
        <param-name>bill</param-name>
        <param-value>ComeBackLenore</param-value>
    </context-param>
    
</web-app>
